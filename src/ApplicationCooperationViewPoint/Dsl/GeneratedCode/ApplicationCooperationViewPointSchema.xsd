        <!-- Id -->
        <xsd:attribute name="Id" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>Instance Guid of this element, needed because SerializeId is set to true.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  
  <!-- CooperationElementHasCooperationSubElement Moniker -->
  <xsd:element name="cooperationElementHasCooperationSubElementMoniker" type="CooperationElementHasCooperationSubElementMoniker" substitutionGroup="core:modelElementMoniker" />
  <xsd:complexType name="CooperationElementHasCooperationSubElementMoniker">
    <xsd:annotation>
      <xsd:documentation>Moniker for CooperationElementHasCooperationSubElement instance.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="core:ModelElementMoniker">
        <xsd:attribute name="Id" type="xsd:string" >
          <xsd:annotation>
            <xsd:documentation>Attribute to store the moniker string.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  
  <!-- CooperationSubElementReferencesTargetCooperationSubElement -->
  <xsd:element name="cooperationSubElementReferencesTargetCooperationSubElement" type="CooperationSubElementReferencesTargetCooperationSubElement" substitutionGroup="core:modelElement" />
  <xsd:complexType name="CooperationSubElementReferencesTargetCooperationSubElement">
    <xsd:complexContent>
      <xsd:extension base="core:ModelElement">
        <xsd:sequence minOccurs="0" maxOccurs="1">
          <!-- Target end CooperationSubElement -->
          <xsd:element ref="cooperationSubElementMoniker" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
              <xsd:documentation>[Target role-player]</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <!-- Id -->
        <xsd:attribute name="Id" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>Instance Guid of this element, needed because SerializeId is set to true.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  
  <!-- CooperationSubElementReferencesTargetCooperationSubElement Moniker -->
  <xsd:element name="cooperationSubElementReferencesTargetCooperationSubElementMoniker" type="CooperationSubElementReferencesTargetCooperationSubElementMoniker" substitutionGroup="core:modelElementMoniker" />
  <xsd:complexType name="CooperationSubElementReferencesTargetCooperationSubElementMoniker">
    <xsd:annotation>
      <xsd:documentation>Moniker for CooperationSubElementReferencesTargetCooperationSubElement instance.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="core:ModelElementMoniker">
        <xsd:attribute name="Id" type="xsd:string" >
